<%= stylesheet_link_tag "login", "data-turbo-track": "reload" %>
<div class="container">
  <h2>Seja Bem-Vindo novamente!</h2>
  <%= form_with(model: Usuario) do |form| %>
  <div class="form-group">
    <%= form.label :email, "Digite seu E-mail:", for:"exampleInputEmail1" %>
    <%= form.email_field :address, class:"form-control", id:"exampleInputEmail1", placeholder:"Seu email" %>
  </div>
  <div class="form-group">
    <%= form.label :password, "Digite sua Senha:", for:"exampleInputPassword1" %>
    <%= form.password_field :password, class:"form-control", id:"exampleInputPassword1", placeholder:"Senha" %>
  </div>
  <div class="form-group">
    <%= form.submit "Logar", class:"btn btn-primary"%>
    <%= link_to 'Back to usuarios',welcome_index_path , class:"btn btn-primary" %>
  </div>
<% end %>
</div>
